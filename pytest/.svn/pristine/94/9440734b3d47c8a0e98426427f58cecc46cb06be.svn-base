from scrapy.item import Item, Field


class Website(Item):

    name = Field()
    url = Field()
    description = Field()

class LXJR_Items(Item):

    amount = Field()
    annualRate = Field()
    term = Field()
    repaymentMethod = Field()
    usages = Field()
    proname = Field()
    maxamount = Field()
    minamount = Field()
    proid = Field()

class LXJR_INV_Items(Item):

    bidder = Field()
    amount = Field()
    bidtime = Field()
    bidtype = Field()
    status = Field()
    proid = Field()

